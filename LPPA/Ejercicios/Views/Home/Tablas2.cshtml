        <tr><td>textarea</td></tr>

        <tr><th rowspan="5">Tabla</th><td>table</td></tr>
        <tr><td>tr</td></tr>
        <tr><td>th</td></tr>
        <tr><td>td</td></tr>
        <tr><td>caption</td></tr>

        <tr><th rowspan="6">Texto</th><td>b</td></tr>
        <tr><td>em</td></tr>
        <tr><td>i</td></tr>
        <tr><td>strong</td></tr>
        <tr><td>sub</td></tr>
        <tr><td>sup</td></tr>
    </table>
</body>
</html>

